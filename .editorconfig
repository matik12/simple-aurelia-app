# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Windows-style newlines with no newline ending every file
[*]
end_of_line = crlf
insert_final_newline = false

# 2 space indentation
[**.*]
indent_style = space
indent_size = 2

[*.scss]
indent_style = space
indent_size = 4